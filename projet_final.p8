--bullets

--shooting
function shoot()
	local newbullet = {
		x=(p.x)*8,
		y=(p.y)*8,
		direction = "up",
		speed=1
 }
	
	 if p.sprite == 34 then  
newbullet.direction = "right"

	elseif p.sprite == 32 then 
newbullet.direction = "up"

	elseif p.sprite == 36  then
newbullet.direction = "left"

	elseif p.sprite == 0 then
newbullet.direction = "down"

end

	add(bullets, newbullet)
	sfx(0)

end

--changement
function update_bullets()

 for b in all(bullets) do
 	if  b.direction == "up" then
	  b.y -= 1
	  b.y -= b.speed
 
	 elseif b.direction == "left" then
 		b.x -= 1
 		b.x -= b.speed
	
 	elseif b.direction == "right" then
 		b.x += 1
 		b.x += b.speed

 	elseif b.direction == "down" then
 		b.y += 1
 		b.y += b.speed            

		end
	end
end


function _update_bullets2()

	for b in all(bullets) do

	 if b.y<-8 then
	 	del(bullets,b)
 	elseif b.y>120 then
 		del(bullets,b)
 	elseif b.x>120 then
 		del(bullets,b)
 	elseif b.x<-8  then 
 		del(bullets,b) 	   
		end
	end
end

function draw_bullets()

	for b in all (bullets) do
		spr(7,b.x,b.y,2,2)
	end
end
