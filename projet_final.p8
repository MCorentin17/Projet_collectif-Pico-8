function player_movement()
	newx=p.x
	newy=p.y
 if (btnp(➡️)) then
 	p.sprite=34
 	newx+=1
 	p.direction="right"
 end
 if (btnp(⬅️)) then
 	p.sprite=36
 	newx-=1
 	p.direction="left"
	end
 if (btnp(⬇️)) then
 	p.sprite=0
  newy+=1
  p.direction="down"
 end
 if (btnp(⬆️)) then
 	p.sprite=32
  newy-=1
  p.direction="up"
 end
  
 if btnp(❎) then
	 shoot()
	 update_bullets()
 end

 collision_e_p()
	interact(newx,newy)
	
 if not check_flag(0,newx,newy) then
 	--on avance
	p.x=newx
	p.y=newy	
 end
end


function interact(x,y)
	if check_flag(1,x,y) then
		pick_up_key(x,y)
		sfx(1)
	elseif check_flag(2,x,y) 
	and	p.keys>0 then
		open_door(x,y)
	end
end
