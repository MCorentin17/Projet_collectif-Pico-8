--collision

--algo
function collisionbullets(a, b)
	if a.x>b.x+8
	or a.y>b.y+8
	or a.x+8<b.x
	or a.y+8<b.y then
		return false
	else
		return true
	end
end

function collisiontuiles(a, b)
	if a.x-7>b.x*8+7
	or a.y-7>b.y*8+7
	or a.x+21<b.x*8+7
	or a.y+18<b.y*8+7 then
		return false
	else
		return true
	end
end

--enemies kill player
function collision_e_p()
 for e in all(enemies) do
 	if collisiontuiles(e,p)==true then
 		p.life-=1
 		--sfx()
 	if p.life<0 then
 		state=2
		end	
		end
	end
		if collisiontuiles(boo,p)==true then
	  p.life-=boo.damage
	  --sfx()
	 if p.life<0 then
	  state=2		
		end
		end
end
	
function _delete_boo()
 for b in all(bullets) do
   if collisionbullets(boo,b)==true then
    --sfx()
    del(bullets,b)
    boo.life-=1
   if boo.life<8 then
    boo.sprite=70
    boo.damage=3
    dx=1.1
    dy=1.5
   end
   if boo.life<0 then
    boo.sprite=088
    boo.damage=0
    score+=100 
   end
   end
 end
end

