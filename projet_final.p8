function _popkey(x,y)
	if (boo.sprite == 088 and
	whk == false)  then
		mset(7,5,3)
	whk = true 
	sfx(4) 				 
	end
end

--passe au sprite d'apres
function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end

--prend la cle
function pick_up_key(x,y)
	next_tile(x,y)
	p.keys+=1
end

--ouvre la porte et enleve une cle 
function open_door(x,y)
	next_tile(x,y)
	p.keys-=1
	sfx(2)
end



